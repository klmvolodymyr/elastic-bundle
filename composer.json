{
    "name": "volodymyr-klymniuk/elastic-bundle",
    "description": "Symfony bundle for working with Elastic Search",
    "type": "symfony-bundle",
    "license": "MIT-advertising",
    "autoload": {
        "psr-4": {
            "VolodymyrKlymniuk\\ElasticBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "authors": [
        {
            "name": "Volodymyr Klymniuk",
            "email": "Volodymyr.Klymniuk@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "volodymyr-klymniuk/std-lib": "^0.0.1",
        "volodymyr-klymniuk/lazy-result-lib": "^0.0.1",
        "doctrine/common": "^3.4",
        "symfony/config": "^6.2",
        "symfony/console": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/http-kernel": "^6.2",
        "ruflin/elastica": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
    },
    "config": {
        "secure-http": false
    },
    "scripts": {
        "test": "phpunit"
    }
}
